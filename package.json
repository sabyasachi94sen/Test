    "js-cookie": "^3.0.1",
    "next": "latest",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^11.3.0",
    "nextjs-progressbar": "^0.0.14",
    "nextjs-routes": "^0.0.18",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
